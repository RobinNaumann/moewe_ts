{
  "name": "moewe",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "main": "dist/moewe.js",
  "types": "dist/moewe.d.ts",
  "scripts": {
    "build": "bun build --target=node ./src/moewe.ts --outfile=dist/moewe.js && bun run build:declaration",
