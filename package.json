  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinNaumann/moewe.git"
  },
  "keywords": [
    "crash-reporting",
    "logging"
  ],
  "version": "0.2.0",
  "author": "Robin Naumann",
  "license": "MIT",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "dependencies": {
    "@preact/signals": "^1.3.0",
    "preact": "^10.23.2",
    "preact-habitat": "^3.3.0"
  }
}
